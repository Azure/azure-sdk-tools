{
  "name": "azuresdkqabot",
  "version": "1.0.2",
  "msteams": {
    "teamsAppId": null
  },
  "description": "Microsoft Teams Toolkit AI Chat Bot Sample with Teams AI Library",
  "engines": {
    "node": "18 || 20 || 22"
  },
  "author": "Microsoft",
  "license": "MIT",
  "main": "./lib/src/index.js",
  "type": "module",
  "scripts": {
    "dev:teamsfx": "env-cmd --silent -f .localConfigs npm run dev",
    "dev:teamsfx:testtool": "env-cmd --silent -f env/.env.testtool npm run dev",
    "dev:teamsfx:launch-teams-testtool": "env-cmd --silent -f env/.env.testtool teamsapptester start",
    "dev": "tsx watch --inspect=9239 ./src/index.ts",
    "build": "tsc --build && shx cp -r ./src/prompts ./lib/src",
    "start": "node ./lib/src/index.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch": "nodemon --exec \"npm run start\""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com"
  },
  "dependencies": {
    "@azure/cognitiveservices-computervision": "^8.2.0",
    "@azure/data-tables": "^13.3.0",
    "@azure/identity": "^4.9.1",
    "@azure/keyvault-secrets": "^4.8.0",
    "@azure/ms-rest-js": "^2.7.0",
    "@azure/storage-blob": "^12.17.0",
    "@microsoft/teams-ai": "^1.5.3",
    "@octokit/rest": "^21.1.1",
    "axios": "^1.13.5",
    "botbuilder": "^4.23.1",
    "express": "^5.2.0",
    "fs-extra": "^11.3.1",
    "strip-ansi": "^7.1.0",
    "winston": "^3.17.0",
    "yaml": "^2.3.4"
  },
  "devDependencies": {
    "@types/express": "^5.0.0",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^20.19.0",
    "env-cmd": "^10.1.0",
    "nodemon": "^3.1.14",
    "shx": "^0.4.0",
    "ts-node": "^10.4.0",
    "tsx": "^4.19.3",
    "typescript": "^5.5.4",
    "vite": "^7.2.2",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.0",
    "@microsoft/teams-app-test-tool": "^0.2.20"
  }
}
