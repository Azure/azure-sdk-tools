    "dist/**/*.d.ts.map",
    "dist/**/*.handlebars",
    "LICENSE",
    "README.md"
  ],
  "bin": {
    "getRepoName": "dist/cli/pipelineCli/getRepoNameCli.js",
    "generateResult": "dist/cli/pipelineCli/generateResultCli.js",
