{
  "name": "eslint-plugin-azure",
  "version": "0.0.0",
  "description": "Linting rules for the JavaScript/TypeScript Azure SDK",
  "keywords": [
    "eslint",
    "eslintplugin",
    "eslint-plugin",
    "azure",
    "azure-sdk",
    "eslint-plugin-azure"
  ],
  "author": "Arpan Laha",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/arpanlaha/eslint-plugin-azure"
  },
  "bugs": {
    "url": "https://github.com/arpanlaha/eslint-plugin-azure/issues"
  },
  "main": "dist/index.js",
  "files": [
    "dist",
    "LICENSE",
    "package.json",
    "README.md"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "clean": "rimraf dist/",
    "format": "prettier --write \"./**/*.{ts,json,md}\"",
    "prebuild": "npm run clean",
    "prepare": "npm test && npm run format && npm run build",
    "test": "ts-mocha -p tsconfig.json tests/**/*.ts",
    "test-temp": "ts-mocha -p tsconfig.json tests/rules/ts-config-strict.ts"
  },
  "dependencies": {
    "@types/eslint": "^4.16.6",
    "@types/estree": "0.0.39",
    "@typescript-eslint/parser": "^1.9.0",
    "typescript": "<3.5.1"
  },
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/mocha": "^5.2.7",
    "chai": "^4.2.0",
    "eslint": "^5.16.0",
    "mocha": "^6.1.4",
    "prettier": "^1.18.2",
    "ts-mocha": "^6.0.0"
  },
  "peerDependencies": {
    "eslint": "^5.16.0"
  },
  "engines": {
    "node": ">=0.10.0"
  }
}
