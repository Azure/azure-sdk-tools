.create-merge table GitHubActionsJob (
    Repository: string,
    WorkflowId: long,
    WorkflowName: string,
    RunId: long,
    RunName: string,
    RunAttempt: long,
    JobId: long,
    JobName: string,
    Status: string,
    Conclusion: string,
    CreatedAt: datetime,
    StartedAt: datetime,
    CompletedAt: datetime,
    NodeId: string,
    HeadSha: string,
    Labels: dynamic,
    RunnerId: long,
    RunnerName: string,
    RunnerGroupId: long,
    RunnerGroupName: string,
    HtmlUrl: string,
    CheckRunUrl: string,
    EtlIngestDate: datetime
) with (folder='', docstring='')

.create-or-alter table GitHubActionsJob ingestion json mapping 'GitHubActionsJob_mapping' ```[
    { "column": "Repository", "path": "$['repository']" },
    { "column": "WorkflowId", "path": "$['workflowId']" },
