{
  "compilerOptions": {
    "allowImportingTsExtensions": true,
    "noEmit": true,
    "lib": [],
    "types": [],
    "skipLibCheck": true,
    "composite": true,
    "esModuleInterop": true,
    "module": "NodeNext",
    "moduleResolution": "NodeNext",
    "target": "ES2024",
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true
  },
  "include": ["./genaisrc/**/*"]
}
