{
  "compilerOptions": {
    "outDir": "./dist",
    "types": ["eslint", "estree"],
    "allowSyntheticDefaultImports": true,
    "allowUnreachableCode": false,
    "allowUnusedLabels": false,
    "esModuleInterop": true,
    "module": "commonjs",
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "pretty": true,
    "sourceMap": true,
    "strict": true,
    "target": "es6",
    "lib": ["es6"],
    "declaration": false,
    "declarationMap": false
  }
}
