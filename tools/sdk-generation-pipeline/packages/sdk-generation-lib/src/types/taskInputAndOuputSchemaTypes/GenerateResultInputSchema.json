{
    "type": "object",
    "properties": {
        "pipelineBuildId": {
            // Azure pipeline buildId.
            "type": "string"
        },
        "logfile": {
            // Absolute path of task log.
            "type": "string"
        },
        "logFilter": {
            // Filter object in string. e.g. "{error:'Error', warning:'Warn'}"
            //  error: RegExp for error keyword
            //  warning: RegExp for warning keyword
            "type": "string"
        },
        "taskName": {
            // Spec repo url in https without auth.
            "type": "string",
            "enum": ["init", "generateAndBuild", "mockTest", "liveTest"]
        },
        "taskExeResult": {
            // Task result string
            "type": "string",
            "enum": ["success", "failure", "timed_out"]
        },
        "taskOutput": {
            // Task result additional object in string.
            "type": "string"
        }
    },
    "required": ["pipelineBuildId", "logfile", "taskName"]
}
