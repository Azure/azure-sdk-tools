{
  "$schema": "./node_modules/@angular/service-worker/config/schema.json",
  "index": "/spa/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.html",
          "/manifest.webmanifest",
          "/*.css",
          "/*.js"
        ]
      }
    },
    {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/**",
          "/*.(svg|cur|jpg|jpeg|png|apng|webp|avif|gif|otf|ttf|woff|woff2|json)"
        ]
      }
    }
  ],
  "dataGroups": [
    {
      "name": "revisioncontent",
      "urls": [
        "https://apiviewstagingtest.com/api/reviews/**/content",
        "https://apiviewuxtest.com/api/reviews/**/content",
        "https://apiview.com/api/reviews/**/content"
      ],
      "cacheConfig": {
        "maxSize": 50,
        "maxAge": "1d",
        "strategy": "performance"
      },
      "version": 2
    }
  ]
}
