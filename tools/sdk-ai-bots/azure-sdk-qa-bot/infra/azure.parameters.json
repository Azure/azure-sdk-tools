{
  "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "resourceBaseName": {
      "value": "${{RESOURCE_SUFFIX}}"
    },
    "azureStorageUrl": {
      "value": "${{AZURE_STORAGE_URL}}"
    },
    "azureTableNameForConversation": {
      "value": "${{AZURE_TABLE_NAME_FOR_CONVERSATION}}"
    },
    "ragApiKey": {
      "value": "${{SECRET_RAG_API_KEY}}"
    },
    "ragEndpoint": {
      "value": "${{RAG_ENDPOINT}}"
    },
    "ragTanentId": {
      "value": "${{RAG_TANENT_ID}}"
    },
    "channelIdForPython": {
      "value": "${{CHANNEL_ID_FOR_PYTHON}}"
    },
    "channelIdForPythonDevInternal": {
      "value": "${{CHANNEL_ID_FOR_PYTHON_DEV_INTERNAL}}"
    },
    "ragTanentIdForPython": {
      "value": "${{RAG_TANENT_ID_FOR_PYTHON}}"
    },
    "webAppSKU": {
      "value": "B3"
    },
    "botDisplayName": {
      "value": "${{TEAMS_BOT_FULL_DISPLAY_NAME}}"
    }
  }
}
