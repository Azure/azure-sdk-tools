    "rimraf": "^3.0.2",
    "jest": "~26.6.3",
    "ts-jest": "~26.5.4",
    "@types/jest": "^25.2.1",
    "typescript": "~4.6.3",
    "ts-node": "~10.7.0",
    "eslint": "^8.16.0",
    "@typescript-eslint/eslint-plugin": "^5.25.0",
