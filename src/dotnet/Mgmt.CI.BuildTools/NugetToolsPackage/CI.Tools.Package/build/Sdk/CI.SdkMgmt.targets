<?xml version="1.0" encoding="utf-8" ?>
<Project>
  <Import Project="..\targets\core\_Directory.Build.targets" />

  <Target Name="PkgEnv">
		<Message Text="CurrentDir=========$(CurrentDir)" Condition="'$(DebugTraceEnv)' == 'true' " />
