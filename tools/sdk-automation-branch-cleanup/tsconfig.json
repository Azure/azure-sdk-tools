{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "lib": [
      "ES6"
    ],
    "declaration": true,
    "sourceMap": true,
    "outDir": "./dist",
    "rootDir": "src",
    "importHelpers": true,
    "downlevelIteration": true,
    "strict": true,
    "noImplicitAny": false,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  }
}
