{
  "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "resourceBaseName": {
      "value": "bot${{RESOURCE_SUFFIX}}"
    },
    "azureComputerVisionEndpoint": {
      "value": "${{AZURE_COMPUTER_VISION_ENDPOINT}}"
    },
    "azureComputerVisionApiKey": {
      "value": "${{SECRET_AZURE_COMPUTER_VISION_API_KEY}}"
    },
    "ragApiKey": {
      "value": "${{SECRET_RAG_API_KEY}}"
    },
    "ragEndpoint": {
      "value": "${{RAG_ENDPOINT}}"
    },
    "ragTanentId": {
      "value": "${{RAG_TANENT_ID}}"
    },
    "channelIdForPython": {
      "value": "${{CHANNEL_ID_FOR_PYTHON}}"
    },
    "ragTanentIdForPython": {
      "value": "${{RAG_TANENT_ID_FOR_PYTHON}}"
    },
